{
  "name": "city-go",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently --kill-others \"npm start\" \"npm run jsonserver\"",
    "test": "ng test --browsers=ChromeHeadless",
    "jsonserver_test": "concurrently --kill-others \"npm run jsonserver\" \"ng test --browsers=ChromeHeadless\"",
    "jsonserver": "json-server -p 3002 --watch db.json",
    "format": "prettier --write \"src/\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint CityGo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/google-maps": "^15.2.9",
    "@angular/material": "^15.2.5",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@types/node": "^20.4.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "prettier-plugin-css-order": "^1.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "tw-colors": "^3.3.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "jasmine-core": "^5.1.2",
    "json-server": "^0.17.4",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "puppeteer": "^22.4.0",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.9.4"
  }
